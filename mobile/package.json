{
  "name": "fleetguard",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "postinstall": "node scripts/postinstall.js",
    "start": "expo start",
    "dev": "expo start --dev-client",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "start:lan": "expo start --lan",
    "start:tunnel": "expo start --tunnel",
    "build:development:ios": "eas build --profile development --platform ios",
    "build:development:android": "eas build --profile development --platform android",
    "build:preview:ios": "eas build --profile preview --platform ios",
    "build:preview:android": "eas build --profile preview --platform android",
    "build:production:ios": "eas build --profile production --platform ios",
    "build:production:android": "eas build --profile production --platform android",
    "build:all:preview": "eas build --profile preview --platform all",
    "build:all:production": "eas build --profile production --platform all",
    "submit:ios": "eas submit --platform ios",
    "submit:android": "eas submit --platform android",
    "lint": "eslint . --ext .js,.jsx",
    "lint:fix": "eslint . --ext .js,.jsx --fix",
    "test": "echo \"No tests yet\" && exit 0"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/stack": "^6.3.20",
    "@sentry/react-native": "~7.2.0",
    "axios": "^1.12.2",
    "cors": "^2.8.5",
    "expo": "~54.0.25",
    "expo-device": "^8.0.9",
    "expo-location": "~19.0.7",
    "expo-notifications": "~0.32.12",
    "expo-secure-store": "^15.0.7",
    "expo-status-bar": "~3.0.8",
    "express": "^5.1.0",
    "lucide-react-native": "^0.554.0",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-native-chart-kit": "^6.12.0",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-maps": "1.20.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "uuid": "^13.0.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "babel-preset-expo": "^54.0.4",
    "eslint": "^9.39.1",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^5.0.0"
  },
  "private": true
